{
    "name": "view-shows",
    "description": "Jordan's implementation of View Shows",
    "version": "1.0.0",
    "homepage": "https://joroze.github.io/ViewShows",
    "repository": {
        "type": "git",
        "url": "https://github.com/Joroze/ViewShows.git"
    },
    "author": {
        "name": "Jordan Rosenberg",
        "url": "http://joroze.com"
    },
    "private": true,
    "dependencies": {
        "axios": "0.18.0",
        "ramda": "0.25.0",
        "react": "16.3.2",
        "react-dom": "16.3.2",
        "react-redux": "5.0.7",
        "react-router-dom": "4.2.2",
        "react-slick": "0.23.1",
        "recompose": "0.27.0",
        "redux": "4.0.0",
        "redux-observable": "0.18.0",
        "rxjs": "5.5.10",
        "slick-carousel": "^1.8.1"
    },
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "NODE_PATH=src/ npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "eslint-config-prettier": "2.9.0",
        "eslint-plugin-prettier": "2.6.0",
        "gh-pages": "1.1.0",
        "node-sass-chokidar": "1.3.0",
        "npm-run-all": "4.1.2",
        "prettier": "1.12.1",
        "react-scripts": "1.1.4",
        "redux-devtools-extension": "2.13.2"
    }
}
